<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="log">
	
	<select id="selectLog" resultType="logVo">
		select * from log
	</select>
	
	<insert id="insertLog">
		insert into log values(#{url}, #{ip}, #{time})
	</insert>
</mapper>